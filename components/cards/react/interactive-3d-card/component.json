{
  "name": "interactive-3d-card",
  "category": "card",
  "framework": "react",
  "tags": ["interactive", "3d", "tilt", "modern"],
  "author": "Ashish-Pandey62",
  "license": "MIT",
  "version": "1.0.0",
  "preview": "An interactive 3D card component with tilt effects on hover",
  "demoUrl": "",
  "props": [
    {
      "name": "width",
      "type": "string",
      "description": "Width of the card (default: '300px')"
    },
    {
      "name": "height",
      "type": "string",
      "description": "Height of the card (default: '400px')"
    },
    {
      "name": "image",
